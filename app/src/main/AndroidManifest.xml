<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.u.app2">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:usesCleartextTraffic="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.u.app2.wal.pop.order.PopupOrderActivity"></activity>
        <activity android:name="com.u.app2.wal.pop.config.PopupConfigActivity"></activity>
        <activity android:name="com.u.app2.wal.pop.vlist.PopupVlistActivity"></activity>
        <activity android:name="com.u.app2.wal.walActivity"></activity>
        <activity android:name="com.u.app2.reg.regActivity" />
        <activity android:name="com.u.app2.errors.errorsActivity" />
        <activity android:name="com.u.app2.MainActivity"></activity>
        <activity  android:name=".SplashActivity" android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>